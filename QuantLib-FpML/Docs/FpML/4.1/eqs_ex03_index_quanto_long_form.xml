                                <representations>
                                        <nonReliance>true</nonReliance>
                                        <agreementsRegardingHedging>true</agreementsRegardingHedging>
                                        <indexDisclaimer>true</indexDisclaimer>
                                        <additionalAcknowledgements>true</additionalAcknowledgements>
                                </representations>
