                                                  <unadjustedDate>2002-10-21</unadjustedDate>
                                                  <unadjustedDate>2004-01-20</unadjustedDate>
                                                  <unadjustedDate>2004-04-22</unadjustedDate>
                                                  <dateAdjustments>
                                                  <businessDayConvention>NotApplicable</businessDayConvention>
                                                  </dateAdjustments>
