                                                <relativeDate>
                                                  <periodMultiplier>3</periodMultiplier>
                                                  <period>D</period>
                                                  <dayType>ExchangeBusiness</dayType>
                                                  <businessDayConvention>NotApplicable</businessDayConvention>
