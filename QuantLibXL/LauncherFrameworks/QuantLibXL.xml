<Framework name="QuantLibXL">

  <GroupBoxes>

    <GroupBox>
      <StartupParameters>
        <KeyValuePair name="Environment" value="Fixed Income" />
        <KeyValuePair name="Workbook Path" value="C:\Projects\QuantLib\trunk\QuantLibXL\Workbooks" />
        <KeyValuePair name="Help File Path" value="C:\Projects\QuantLib\trunk\QuantLibXL\Docs" />
        <KeyValuePair name="Metadata Path" value="C:\Projects\QuantLib\trunk\QuantLibAddin\gensrc\metadata" />
      </StartupParameters>
    </GroupBox>

    <GroupBox text="Settings">
      <StartupParameters>
        <CheckBox text="Set Evaluation Date" checked="False" enabled="True" />
        <DateTimePicker label="Evaluation Date" value="10/12/2011" enabled="True" />
        <ComboBox label="Default Currency" selectedItem="EUR" enabled="True">
          <Items>
            <Item>EUR</Item>
            <Item>GBP</Item>
            <Item>USD</Item>
            <Item>CHF</Item>
            <Item>JPY</Item>
          </Items>
        </ComboBox>
        <ComboBox label="Initialization Data Source" selectedItem="XML" enabled="True">
          <Items>
            <Item>Excel</Item>
            <Item>XML</Item>
          </Items>
        </ComboBox>
        <CheckBox text="Open Main Checks" checked="True" enabled="True" />
      </StartupParameters>
    </GroupBox>

    <GroupBox text="Index Fixings">
      <StartupParameters>
        <CheckBox text="Update Indexes Time Series" checked="False" enabled="True" />
        <CheckBox text="Load Indexes Time Series" checked="False" enabled="True" />
      </StartupParameters>
    </GroupBox>

    <GroupBox text="Curves">
      <StartupParameters>
        <CheckBox text="Bootstrap Yield Curves" checked="False" enabled="True" />
        <CheckBox text="Bootstrap Cap Volatilities" checked="False" enabled="True" />
        <CheckBox text="Create Swaption Volatility Cube" checked="False" enabled="True" />
      </StartupParameters>
    </GroupBox>

    <GroupBox text="RealTime Data">
      <StartupParameters>
        <CheckBox text="Switch to Static Market Data" checked="False" enabled="True" />
        <ComboBox label="Feed Use" selectedItem="Reuters" enabled="True">
          <Items>
            <Item>Reuters</Item>
            <Item>Bloomberg</Item>
          </Items>
        </ComboBox>
      </StartupParameters>
    </GroupBox>

  </GroupBoxes>
  
  <Components>
    <Component path="C:\Projects\QuantLib\trunk\ObjectHandler\xll\ObjectHandler-xll-vc90-mt-1_7_0.xll" />
    <Component path="C:\Projects\QuantLib\trunk\QuantLibXL\xll\QuantLibXLDynamic-vc90-mt-1_7_0.xll" />
    <Component path="C:\Projects\QuantLib\trunk\QuantLibXL\framework2\QuantLibXL.xla" />
  </Components>

  <EnvironmentVariables>
    <EnvironmentVariable name="notUsed" value="test" />
  </EnvironmentVariables>

</Framework>